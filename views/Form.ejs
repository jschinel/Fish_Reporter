<!DOCTYPE html>
<html lang="en">
<head>
    <%-include('./partials/head.ejs')%>
    <link rel="stylesheet" href="/styles/addform.css">
</head>

<body>
    <%-include('./partials/top.ejs')%>
    <%-include('./partials/nav.ejs')%> 
    <main class ='itemList'>
        <form action='/Catalog/postnew' method="POST">
            <label for="Item_Number">Item Number</label>
            <input type="Number" name="Item_Number" id="Item_Number" >
            <label for="Name">Name</label>
            <input type="String" name="Name" id="Name">
            <label for="Description">Description</label>
            <input type="String" name="Description" id="Description">
            <label for="Rating">Rating</label>
            <input type="Number" name="Rating" id="Rating" min="0" max="10">
            <label for="Price">Price</label>
            <input type="Number" name="Price" id="Price" min="0">
            <label for="Quantity">Quantity</label>
            <input type="Number" name="Quantity" id="Quantity" min="0">
            <label for="Picture">Picture</label>
            <input type="String" name="Picture" id="Picture">
            <input class = "submit" type="submit" value="submit">
        </form>
    </main>
</body>

