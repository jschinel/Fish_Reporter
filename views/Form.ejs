            <label for="Location_Type">Location Type</label>
            <input type="String" name="Location_Type" id="Location_Type">
            <label for="Name">Name</label>
            <input type="String" name="Name" id="Name">
            <label for="City">City</label>
            <input type="String" name="City" id="City">
            <label for="Fish">Fish</label>
            <input type="String" name="Fish" id="Fish" >
            <label for="Fish_Caught">Fish Caught</label>
            <input type="Number" name="Fish_Caught" id="Fish_Caught" min="0" >
            <label for="Picture">Picture</label>
